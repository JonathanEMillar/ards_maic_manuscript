@ARTICLE{Martucci2020,
  title     = "Identification of a circulating {miRNA} signature to stratify
               acute respiratory distress syndrome patients",
  author    = "Martucci, Gennaro and Arcadipane, Antonio and Tuzzolino, Fabio
